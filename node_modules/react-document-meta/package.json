{
  "name": "react-document-meta",
  "version": "3.0.0-beta.2",
  "description": "Declarative, nested and stateful HTML document meta tags for React",
  "main": "./dist/index",
  "scripts": {
    "dev": "node ./example/client-side/server.js",
    "dev:ssr": "babel-node ./example/server-side/server.js",
    "lint": "eslint ./lib ./example",
    "test": "NODE_ENV=test jest --coverage lib/",
    "test:watch": "NODE_ENV=test jest --watch lib/",
    "test:dist": "NODE_ENV=test jest dist/"
  },
  "keywords": [
    "react",
    "meta",
    "html",
    "document",
    "tags",
    "react-component"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-istanbul": "^0.12.2",
    "babel-jest": "^22.0.4",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "coveralls": "^2.13.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.15.2",
    "jest": "^22.0.4",
    "jsdom": "^9.12.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^1.3.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "dependencies": {
    "prop-types": "^15.5.8",
    "react-side-effect": "^1.1.0"
  },
  "repository": "kodyl/react-document-meta",
  "author": "Daniel Juhl",
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "lib/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 80,
        "statements": 80
      }
    },
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*[-.]test.js?(x)",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "setupFiles": [
      "<rootDir>/lib/__tests__/testdom.js"
    ]
  }
}
